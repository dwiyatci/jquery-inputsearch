{
  "name"           : "jquery-inputsearch",
  "version"        : "0.3.0",
  "description"    : "jQuery plugin for automagically transforming input[type=\"text\"] elements into input[type=\"search\"]-like elements.",
  "main"           : "dist/jquery.inputsearch.min.js",
  "keywords"       : [
    "jquery-plugin",
    "ecosystem:jquery",
    "input",
    "input-text",
    "input-search",
    "inputsearch",
    "searchinput",
    "html5-element"
  ],
  "homepage"       : "https://github.com/dwiyatci/jquery-inputsearch",
  "bugs"           : {
    "url"  : "https://github.com/dwiyatci/jquery-inputsearch/issues",
    "email": "dwiyatci@mail.com"
  },
  "license"        : "SEE LICENSE IN LICENSE.txt",
  "author"         : {
    "name" : "Glenn Dwiyatcita",
    "email": "dwiyatci@mail.com",
    "url"  : "http://tiny.cc/dwiyatci"
  },
  "dependencies"   : {
    "jquery": "2.2.2"
  },
  "devDependencies": {
    "babel-loader"        : "6.2.4",
    "babel-polyfill"      : "6.7.4",
    "babel-preset-es2015" : "6.6.0",
    "babel-preset-stage-2": "6.5.0",
    "jscs"                : "2.11.0",
    "rimraf"              : "2.5.2",
    "updtr"               : "0.1.10",
    "webpack"             : "1.12.14",
    "webpack-dev-server"  : "1.14.1"
  },
  "scripts"        : {
    "setup"        : "npm install rimraf updtr webpack webpack-dev-server jscs -g",
    "install:clean": "rimraf node_modules && npm install",
    "clean"        : "rimraf dist",
    "webpack"      : "webpack",
    "watch"        : "webpack -w",
    "start"        : "webpack-dev-server --inline --hot",
    "jscs"         : "jscs src",
    "build"        : "npm run jscs && npm run clean && webpack",
    "update"       : "updtr",
    "update:global": "npm run setup"
  },
  "repository"     : {
    "type": "git",
    "url" : "https://github.com/dwiyatci/jquery-inputsearch"
  }
}
